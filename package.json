{
  "name": "gemini-meeting-pilot",
  "private": true,
  "version": "0.0.0",
  "main": "src-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && set ELECTRON_RUN_AS_NODE=&& npx electron .\"",
    "electron:builder": "electron-builder"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "electron-is-dev": "^3.0.1",
    "express": "^5.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "sqlite3": "^5.1.7"
  },
  "build": {
    "appId": "com.meetingpilot.app",
    "productName": "Meeting Pilot",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "src-electron/**/*",
      "backend/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^33.0.0",
    "electron-builder": "^26.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.3"
  }
}